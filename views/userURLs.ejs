<!DOCTYPE html>

<html lang="en">
<head>
    <%- include ./partials/_header.ejs %>
</head>
<body>
    <table style="width: 80%">
        <tr>
            <th>Short URL</th>
            <th>Long URL</th>
        </tr>
        <% for(let shortURL in urls) { %>
            <tr>
                <td><%= shortURL %></td>
                <td><%= urls[shortURL] %>
                    <form method="POST" action=<%= "/" + user_id + "/urls/delete" %>
                        <input type="submit" value="Delete">
                    </form>
                </td>
            </tr>
            <% } %>
        </table>
</body>
</html>